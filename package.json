{
  "name": "transcribe",
  "version": "1.0.0",
  "description": "A website to learn to read manuscripts",
  "main": "index.tsx",
  "scripts": {
    "start": "vite",
    "start:dev": "npm-run-all --parallel -r start types:watch",
    "build": "vite build",
    "test": "jest --coverage",
    "test-u": "npm run test -- -u",
    "eslint": "eslint . --ext .ts,.tsx --cache --cache-strategy content",
    "eslint-fix": "eslint --fix . --ext .ts,.tsx --cache --cache-strategy content",
    "types:watch": "tsc --noEmit --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JacobWPeterson/transcribe"
  },
  "author": "Jacob W. Peterson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JacobWPeterson/transcribe/issues"
  },
  "homepage": "https://github.com/JacobWPeterson/transcribe#readme",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "mirador": "^4.0.0-alpha.2",
    "npm-run-all": "^4.1.5",
    "pluralize": "^8.0.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "classnames": "2.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-vitest": "^0.5.4",
    "jest": "^27.4.5",
    "jest-css-modules-transform": "^4.4.2",
    "jest-svg-transformer": "^1.0.0",
    "postcss": "8.4.38",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "9.5.13",
    "sass": "^1.77.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0",
    "vite": "^5.2.11",
    "vite-plugin-svgr": "4.2.0",
    "vitest": "^1.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.svg$": "jest-svg-transformer",
      ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform"
    }
  }
}